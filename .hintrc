{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "typescript-config/is-valid": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "font-synthesis",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}